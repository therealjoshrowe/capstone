<Window x:Name="MainWindow1" x:Class="Easy_Nex_Maukup.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Easy_Nex_Maukup"
        mc:Ignorable="d"
        Title="Easy Nex" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="Taxa_Data" HorizontalAlignment="Left" Height="72" Margin="10,58,0,0" TextWrapping="Wrap" Text="Enter Taxa Data" VerticalAlignment="Top" Width="169" TextChanged="Taxa_Data_TextChanged"/>
        <TextBlock x:Name="TaxaBlockHeader" HorizontalAlignment="Left" Margin="10,23,0,0" TextWrapping="Wrap" Text="Taxa" VerticalAlignment="Top" Height="30" Width="68"/>
        <Button x:Name="WriteToFileButton" Content="Write to file" HorizontalAlignment="Left" Margin="384,43,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="72" Margin="10,191,0,0" TextWrapping="Wrap" Text="Enter Matrix:&#xD;&#xA;Taxon Character-Sequence" VerticalAlignment="Top" Width="169" TextChanged="textBox_TextChanged"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,152,0,0" TextWrapping="Wrap" Text=" Characters" VerticalAlignment="Top" Height="34" Width="68"/>
    </Grid>
</Window>
